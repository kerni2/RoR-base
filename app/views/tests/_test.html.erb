
<tr>
  <td><a href="/tests/<%= test.id %>/questions/"><%= test.title %></a></td>
  <td><%= test_level(test) %></td>
  <td><%= test_count_questions(test) %></td>
  <td><%= link_to 'Show', test_path(test) %></td>
  <td><%= link_to 'Edit', edit_test_path(test) %></td>
  <td><%= link_to 'Delete', 
                  test_path(test), 
                  method: :delete, 
                  data: { confirm: 'Are you sure?' } %></td>
</tr>


