<h3>TEST:</h3>
<p>
  <strong> Title:</strong>
  <%= @test.title %>
</p>
<p>
  <strong> Level:</strong>
  <%= @test.level %>
</p>
<table>
  <tr>
    <th>Question</th>
    <th colspan='3'>Actions</th>
  <tr/>
  <%= render @test.questions %>
</table>


<p><%= link_to 'New question', new_admin_test_question_path(@test) %></p>
<p><%= link_to 'Edit test', edit_admin_test_path(@test) %></p>
<p><%= link_to 'Back to tests', admin_tests_path %></p>